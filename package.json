{
    "name": "kagsa",
    "displayName": "KAGSA",
    "description": "KAGSA programming language syntax highlighting for VSCode.",
    "publisher":"kagsa",
    "icon": "images/icon.png",
    "version": "1.1.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Themes",
        "Extension Packs",
        "Linters"
    ],
    "contributes": {
        "languages": [{
            "id": "kagsa",
            "aliases": ["Kagsa", "kagsa"],
            "extensions": [".kg"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "kagsa",
            "scopeName": "source.kg",
            "path": "./syntaxes/kagsa.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git", 
        "url": "https://github.com/kagsa/kagsa"
    }
}
